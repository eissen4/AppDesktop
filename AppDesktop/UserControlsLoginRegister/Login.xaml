<UserControl x:Class="AppDesktop.UserControlsLoginRegister.Login"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AppDesktop.UserControlsLoginRegister"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Margin="10">
            <Label Margin="10">Usuario:</Label>
            <TextBox x:Name="usernameTxt" Margin="10"></TextBox>
            <Label Margin="10">Contraseña:</Label>
            <TextBox x:Name="passwordTxt" Margin="10"></TextBox>
            <Button x:Name="sendBtn" Margin="10" Click="sendBtn_Click">Enviar</Button>
        </StackPanel>
            
    </Grid>
</UserControl>
